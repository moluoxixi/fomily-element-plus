{
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "preserve",
    "lib": [
      "ESNext"
    ],
    "baseUrl": ".",
    "paths": {
      "@moluoxixi/prototypes": [
        "../prototypes"
      ],
      "@moluoxixi/prototypes/*": [
        "../prototypes/*"
      ],
      "@moluoxixi/element": [
        "../element"
      ],
      "@moluoxixi/element/*": [
        "../element/*"
      ],
      "@moluoxixi/builtins": [
        "../builtins"
      ],
      "@moluoxixi/builtins/*": [
        "../builtins/*"
      ],
      "@moluoxixi/renderer": [
        "../renderer"
      ],
      "@moluoxixi/renderer/*": [
        "../renderer/*"
      ],
      "@moluoxixi/setters": [
        "../setters"
      ],
      "@moluoxixi/setters/*": [
        "../setters/*"
      ],
      "@moluoxixi/settings-form": [
        "../settings-form"
      ],
      "@moluoxixi/settings-form/*": [
        "../settings-form/*"
      ]
    },
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
